pipeline {
  agent {
    label 'Linux1'
  }
  stages {
    stage('PREPARATION') {
      steps {
        sh '''
        whoami
        '''
      }
    }
    stage('Runing APP') {
      steps {
        sh '''
        docker-compose up --force-recreate --build --no-cache -d
        docker image prune -f
        '''
      }
    }
  }  
}
